    <blocked block="false" dx="2" dy="4"/>
    <blocked block="false" dx="3" dy="4"/>
    <blocked block="false" dx="4" dy="4"/>
    <blocked block="false" dx="4" dy="3"/>
    <blocked block="false" dx="4" dy="2"/>
    <blocked block="false" dx="4" dy="1"/>
